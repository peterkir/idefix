<?xml version="1.0" encoding="UTF-8"?>
<setup:ProductCatalog
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    name="com.daimler.products"
    label="Daimler Products">
  <setupTask
      xsi:type="setup:CompoundTask"
      name="REDIRECTION catalogProduct">
    <setupTask
        xsi:type="setup:EclipseIniTask"
        option="-Doomph.redirection.klibProductCatalog"
        value="=index:/redirectable.products.setup->http://peterkir.github.io/idefix/bootstrap/daimler.cec/catalogProducts.setup"
        vm="true"/>
  </setupTask>
  <setupTask
      xsi:type="setup:VariableTask"
      name="oomph.update.url"
      value="http://download.eclipse.org/oomph/updates/release"
      defaultValue=""/>
  <product href="http://peterkir.github.io/idefix/bootstrap/daimler.cec/product_idefix.cec.oxygen.setup#/"/>
  <product href="http://peterkir.github.io/idefix/bootstrap/daimler.cec/product_idefix.cec.neon.setup#/"/>
  <product href="http://peterkir.github.io/idefix/bootstrap/daimler.cec/product_idefix.cec.neon_bndtools_3_3.setup#/"/>
  <product href="http://peterkir.github.io/idefix/bootstrap/daimler.cec/product_idefix.appdev.neon.setup#/"/>
</setup:ProductCatalog>
